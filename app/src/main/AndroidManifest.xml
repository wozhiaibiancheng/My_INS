<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.edu.unimelb.student.group55.my_ins">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginNRegister.LoginActivity"></activity>
        <activity android:name=".LoginNRegister.RegisterActivity"></activity>
        <activity android:name=".Discovery.DiscoverActivity"></activity>
        <activity android:name=".Feed.FeedActivity"></activity>
        <activity android:name=".Profile.ProfileActivity"></activity>
        <activity android:name=".Upload.UploadActivity"></activity>
        <activity android:name=".Home.HomeActivity" android:exported="true"></activity>
        <activity android:name=".PhotoNLibrary.BottomDialog"></activity>
        <activity android:name=".PhotoNLibrary.TakingPhotoActivity"></activity>
        <activity android:name=".PhotoNLibrary.PhotoLibraryActivity"></activity>
    </application>

</manifest>